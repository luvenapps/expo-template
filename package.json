{
  "name": "template-starter",
  "version": "1.0.0",
  "license": "MIT",
  "author": "LuvenApps <github@luvenapps.com>",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run troubleshoot",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:https": "npx expo export --platform web && cp public/firebase-messaging-sw.js dist/ && npx serve -s dist -l 3000 --ssl-cert ./certs/localhost+2.pem --ssl-key ./certs/localhost+2-key.pem",
    "pretest": "npm run type-check",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "doctor": "expo-doctor --verbose",
    "start:ci": "expo start --dev-client --no-dev --minify --port 8081 --clear",
    "e2e:ios": "maestro test .maestro/flows/smoke.ios.yaml",
    "e2e:android": "maestro test .maestro/flows/smoke.android.yaml",
    "commit": "cz",
    "troubleshoot": "node scripts/troubleshoot.mjs",
    "postinstall": "node scripts/postinstall.js",
    "db:migrate": "node scripts/generate-migrations.mjs",
    "db:generate": "drizzle-kit generate --config=drizzle.config.sqlite.ts",
    "db:generate:postgres": "node scripts/generate-postgres-migration.mjs",
    "db:generate-rls": "node scripts/generate-rls-policies.mjs",
    "db:push": "drizzle-kit push --config=drizzle.config.sqlite.ts",
    "db:clean": "node scripts/clean-migrations.mjs",
    "supabase:push": "npx supabase db push",
    "supabase:dev": "node scripts/supabase-dev.mjs",
    "supabase:deploy": "node scripts/supabase-deploy.mjs",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:prod:ios": "eas build --platform ios --profile production",
    "build:prod:android": "eas build --platform android --profile production",
    "submit:prod:ios": "eas submit --platform ios --profile production",
    "submit:prod:android": "eas submit --platform android --profile production",
    "version:bump": "node scripts/bump-version.mjs"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^23.5.0",
    "@react-native-firebase/app": "^23.5.0",
    "@react-native-firebase/in-app-messaging": "^23.5.0",
    "@react-native-firebase/messaging": "^23.5.0",
    "@supabase/supabase-js": "^2.76.0",
    "@tamagui/babel-plugin": "^1.135.2",
    "@tamagui/core": "^1.135.2",
    "@tamagui/font-inter": "^1.135.2",
    "@tamagui/lucide-icons": "^1.135.2",
    "@tamagui/metro-plugin": "^1.135.2",
    "@tanstack/query-async-storage-persister": "^5.90.8",
    "@tanstack/query-sync-storage-persister": "^5.90.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-persist-client": "^5.90.8",
    "dayjs": "^1.11.19",
    "drizzle-orm": "^0.44.6",
    "expo": "~54.0.31",
    "expo-apple-authentication": "~8.0.8",
    "expo-application": "~7.0.8",
    "expo-background-task": "~1.0.10",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.8",
    "expo-device": "~8.0.10",
    "expo-linking": "~8.0.11",
    "expo-localization": "~17.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-task-manager": "~14.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^11.0.2",
    "i18next": "^24.2.3",
    "idb-keyval": "^6.2.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.7.4",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "0.21.1",
    "tamagui": "^1.135.2",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@expo/fingerprint": "^0.15.1",
    "@expo/ngrok": "^4.1.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.38.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "expo-dev-client": "~6.0.20",
    "expo-doctor": "^1.17.9",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.16",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "semver": "^7.6.3",
    "serve": "^14.2.5",
    "supabase": "^2.54.11",
    "typescript": "~5.9.2"
  },
  "private": true,
  "overrides": {
    "react-server-dom-webpack": "19.1.2",
    "react-dom": "19.1.0",
    "react": "19.1.0",
    "react-native-web": "0.21.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ],
    "*.*": [
      "npm run format:check"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    }
  },
  "x-template": {
    "bundleIdBase": "com.luvenapps",
    "templateName": "template-starter"
  },
  "expo": {
    "autolinking": {
      "legacy_shallowReactNativeLinking": true
    }
  }
}
