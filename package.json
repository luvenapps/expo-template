{
  "name": "template-starter",
  "version": "1.0.0",
  "license": "MIT",
  "author": "LuvenApps <github@luvenapps.com>",
  "main": "expo-router/entry",
  "scripts": {
    "prestart": "npm run troubleshoot",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "doctor": "expo-doctor --verbose",
    "start:ci": "expo start --dev-client --no-dev --minify --port 8081 --clear",
    "e2e:ios": "maestro test .maestro/flows/smoke.ios.yaml",
    "e2e:android": "maestro test .maestro/flows/smoke.android.yaml",
    "commit": "cz",
    "troubleshoot": "node scripts/troubleshoot.js",
    "postinstall": "node scripts/postinstall.js",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:push": "drizzle-kit push:sqlite"
  },
  "dependencies": {
    "@tamagui/babel-plugin": "^1.135.2",
    "@tamagui/core": "^1.135.2",
    "@tamagui/font-inter": "^1.135.2",
    "@tamagui/lucide-icons": "^1.135.2",
    "@tamagui/metro-plugin": "^1.135.2",
    "@tanstack/react-query": "^5.90.5",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "eslint": "^9.37.0",
    "expo": "~54.0.12",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.15",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.12",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.12",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "0.21.1",
    "tamagui": "^1.135.2",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@expo/fingerprint": "^0.15.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "expo-doctor": "^1.17.9",
    "husky": "^9.1.7",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "overrides": {
    "react": "19.1.0",
    "react-native-web": "0.21.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ],
    "*.*": [
      "npm run format:check"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    }
  },
  "x-template": {
    "bundleIdBase": "com.luvenapps",
    "templateName": "template-starter"
  }
}
