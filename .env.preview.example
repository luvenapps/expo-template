# Supabase Preview (Staging) Configuration
# Copy this file to `.env.preview` and replace the placeholders.

EXPO_PUBLIC_SUPABASE_URL=https://your-preview-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-preview-anon-key

# Analytics destination used in preview builds (optional locally, required in CI)
EXPO_PUBLIC_ANALYTICS_ENDPOINT=https://us.i.posthog.com
EXPO_PUBLIC_ANALYTICS_WRITE_KEY=phc_your_preview_key

# Helps screens/debug logs know which environment is running
EXPO_PUBLIC_ENV=preview
